<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CD2222"
    tools:context=".StudentProfile">

    <Button
        android:id="@+id/sProfileDash"
        android:layout_width="145dp"
        android:layout_height="48dp"
        android:layout_marginBottom="30dp"
        android:alpha="1"
        android:backgroundTint="#CEB326"
        android:fontFamily="@font/orienta"
        android:text="Dashboard"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/sProfileEdit"
        app:layout_constraintStart_toStartOf="@+id/sProfileEdit"
        app:layout_constraintTop_toBottomOf="@+id/sProfileEdit" />

    <Button
        android:id="@+id/sProfileEdit"
        android:layout_width="138dp"
        android:layout_height="50dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#CEB326"
        android:fontFamily="@font/orienta"
        android:text="Edit"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/sProfileDash"
        app:layout_constraintEnd_toEndOf="@+id/sProfilePass"
        app:layout_constraintStart_toStartOf="@+id/sProfilePass"
        app:layout_constraintTop_toBottomOf="@+id/sProfilePass"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/sProfilePass"
        android:layout_width="290dp"
        android:layout_height="45dp"
        android:layout_marginBottom="23dp"
        android:background="#CEB326"
        android:fontFamily="@font/orienta"
        android:text="**********"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/sProfileEdit"
        app:layout_constraintEnd_toEndOf="@+id/sProfileEmail"
        app:layout_constraintStart_toStartOf="@+id/sProfileEmail"
        app:layout_constraintTop_toBottomOf="@+id/sProfileEmail" />

    <TextView
        android:id="@+id/sProfileEmail"
        android:layout_width="290dp"
        android:layout_height="45dp"
        android:layout_marginBottom="23dp"
        android:background="#CEB326"
        android:fontFamily="@font/orienta"
        android:text="Email"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/sProfilePass"
        app:layout_constraintEnd_toEndOf="@+id/sProfileUser"
        app:layout_constraintStart_toStartOf="@+id/sProfileUser"
        app:layout_constraintTop_toBottomOf="@+id/sProfileUser" />

    <TextView
        android:id="@+id/sProfileUser"
        android:layout_width="290dp"
        android:layout_height="45dp"
        android:layout_marginBottom="22dp"
        android:background="#CEB326"
        android:fontFamily="@font/orienta"
        android:text="UserName"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/sProfileEmail"
        app:layout_constraintEnd_toEndOf="@+id/sProfileName"
        app:layout_constraintStart_toStartOf="@+id/sProfileName"
        app:layout_constraintTop_toBottomOf="@+id/sProfileName" />

    <TextView
        android:id="@+id/Profile"
        android:layout_width="257dp"
        android:layout_height="39dp"
        android:layout_marginStart="77dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="16dp"
        android:background="#CEB326"
        android:backgroundTint="#CEB326"
        android:fontFamily="@font/orienta"
        android:text="Profile"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sProfileName"
        android:layout_width="290dp"
        android:layout_height="45dp"
        android:layout_marginBottom="23dp"
        android:background="#CEB326"
        android:fontFamily="@font/orienta"
        android:text="Name"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/sProfileUser"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="176dp"
        android:layout_height="150dp"
        android:layout_marginBottom="37dp"
        app:layout_constraintBottom_toTopOf="@+id/sProfileName"
        app:layout_constraintEnd_toEndOf="@+id/Profile"
        app:layout_constraintStart_toStartOf="@+id/Profile"
        app:layout_constraintTop_toBottomOf="@+id/Profile"
        app:srcCompat="@drawable/tarpmascot" />
</androidx.constraintlayout.widget.ConstraintLayout>